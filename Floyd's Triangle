import java.util.Scanner;
public class Main{
    public static void main(String[]args) {
        Scanner in = new Scanner(System.in);
        int rows= in.nextInt();
        int number = 1;
        int i,j;
        for (i = 1; i <= rows; i++) {
            for (j = 1; j <= i; j++) {
                System.out.print(number + " ");
                number++;
            }
            System.out.println();
        }
    }
}
