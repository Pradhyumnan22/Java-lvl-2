import java.util.*;
public class Main{
    public static void main(String[]args){
        Scanner in = new Scanner(System.in);
        int n = in.nextInt();
        int ld,rev=0;
        int temp=n;
        while(temp!=0){
            ld=temp%10;
            rev=rev*10+ld;
            temp=temp/10;
        }
        System.out.println(rev);
    }
}
