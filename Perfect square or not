import java.util.Scanner;
public class Main{
    public static void main(String[]args){
        Scanner in = new Scanner(System.in);
        int n=in.nextInt();
        int sq = (int) Math.sqrt(n);
        if(sq*sq==n){
            System.out.println("Perfect Square");
        }
        else{
            System.out.println("Not Perfect");
        }
    }
}
