import java.util.Scanner;
public class Main{
    public static void main(String[]args){
        Scanner in = new Scanner(System.in);
        int n=in.nextInt();
        int temp=n;
        int ld;
        int sum=0;
        ld=temp%10;
        while(temp>=10){
            temp=temp/10;
        }
        sum=temp+ld;
        System.out.println(sum);
    }
}
