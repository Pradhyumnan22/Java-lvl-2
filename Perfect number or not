import java.util.Scanner;
public class Main {
    public static void main(String[] args) {
        Scanner in = new Scanner(System.in);
        int num= in.nextInt();
        int sum = 0;
        int i;
        for (i = 1; i <= num/2; i++) {
            if (num%i == 0) {
                sum = sum+i;
            }
        }
        if (sum == num) {
            System.out.println("Perfect Number.");
        }
        else{
            System.out.println("Not Perfect Number.");
        }
    }
}
